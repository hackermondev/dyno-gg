    "winston": "^3.0.0-rc4"
  },
  "devDependencies": {
    "@types/mongodb": "^3.0.10",
    "@types/node": "^9.6.2",
    "nodemon": "^1.17.3",
    "tslint": "^5.9.1",
    "typescript": "^2.8.1"
  }
}
